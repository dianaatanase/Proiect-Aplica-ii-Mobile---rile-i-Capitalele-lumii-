<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5d6ff">

    <TextView
        android:id="@+id/profile_fragment_tv_my_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="24dp"
        android:gravity="center_horizontal"
        android:text="@string/profile_fragment_tv_my_profile"
        android:textAlignment="center"
        android:textColor="#570b83"
        android:textStyle="bold"
        android:textSize="24dp"
        android:fontFamily="sans-serif-light"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/profile_fragment_tv_user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/profile_fragment_tv_my_profile"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:gravity="start"
        android:textAlignment="textStart"
        android:textColor="#570b83"
        android:fontFamily="sans-serif-light"
        android:textStyle="bold"
        android:layout_marginStart="16dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true" />

    <View
        android:id="@+id/profile_fragment_view_2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/profile_fragment_tv_user"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="8dp"
        android:background="#e285ff"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/profile_fragment_tv_edit_profile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/profile_fragment_view_2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:gravity="start"
        android:text="@string/fragment_profile_reviews"
        android:textAlignment="textStart"
        android:textColor="#570b83"
        android:fontFamily="sans-serif-light"
        android:textStyle="bold"
        android:layout_marginStart="16dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true" />

    <View
        android:id="@+id/profile_fragment_view_3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/profile_fragment_tv_edit_profile"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="8dp"
        android:background="#e285ff"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true" />


    <TextView
        android:id="@+id/profile_fragment_tv_rate_our_app"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/profile_fragment_view_3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:gravity="start"
        android:text="@string/profile_fragment_tv_rate_app"
        android:textAlignment="textStart"
        android:textColor="#570b83"
        android:fontFamily="sans-serif-light"
        android:textStyle="bold"
        android:layout_marginStart="16dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true" />

    <View
        android:id="@+id/profile_fragment_view_4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/profile_fragment_tv_rate_our_app"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="8dp"
        android:background="#e285ff"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true" />
    <TextView
        android:id="@+id/profile_fragment_tv_send_feedback"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/profile_fragment_view_4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:gravity="start"
        android:text="@string/profile_fragment_tv_send_feedback"
        android:textAlignment="textStart"
        android:textColor="#570b83"
        android:fontFamily="sans-serif-light"
        android:textStyle="bold"
        android:layout_marginStart="16dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true" />

    <View
        android:id="@+id/profile_fragment_view_5"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/profile_fragment_tv_send_feedback"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="8dp"
        android:background="#e285ff"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/profile_fragment_tv_log_out"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/profile_fragment_view_5"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:gravity="start"
        android:text="@string/profile_fragment_tv_log_out"
        android:textAlignment="textStart"
        android:textColor="#570b83"
        android:fontFamily="sans-serif-light"
        android:textStyle="bold"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="16dp" />

    <View
        android:id="@+id/profile_fragment_view_6"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@id/profile_fragment_tv_log_out"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="8dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="8dp"
        android:background="#e285ff"
        android:layout_alignParentEnd="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>